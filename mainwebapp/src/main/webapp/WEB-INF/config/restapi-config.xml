<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

  <!-- REST API resources -->
  <bean id="storageProviderRest" class="org.duraspace.mainwebapp.rest.StorageProviderRest" />
  
  <bean id="storageProviderResource" class="org.duraspace.mainwebapp.rest.StorageProviderResource" >
    <property name="storageManager" ref="storageMgr" />
  </bean>
  
  <bean id="serviceResource" class="org.duraspace.mainwebapp.rest.ServiceRest" />

</beans>